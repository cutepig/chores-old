{
  "name": "chores",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "bash -c \"node-sass --include-path node_modules --include-path=./src --importer node_modules/node-sass-import-once/index.js src/chores/index.scss > public/index.css",
    "watch:css": "chokidar \"src/chores/**/*.scss\" --initial --verbose --command \"npm run build:css\"",
    "build:assets": "bash -c \"cp -r src/assets/* public\"",
    "watch:assets": "chokidar \"src/assets/**/*\" --initial --verbose --command \"npm run build:assets\"",
    "build:js": "bash -c \"NODE_PATH=./src browserify -t babelify --extension=.jsx src/chores/index.js -o public/index.js",
    "watch:js": "bash -c \"NODE_PATH=./src watchify -t babelify --extension=.jsx src/chores/index.jsx -o public/index.js\"",
    "serve": "http-server public -p 9966",
    "clean": "bash -c \"[ -d public ] && rm -r public\"",
    "build": "npm run clean && mkdir public && npm run build:css && npm run build:assets && npm run build:js",
    "watch": "bash -c \"npm run watch:css & npm run watch:assets & npm run watch:js",
    "start": "bash -c \"npm run clean && mkdir public && npm run serve & npm run watch\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bourbon": "^4.2.7",
    "browserify": "^13.1.0",
    "dialog-polyfill": "^0.4.3",
    "firebase": "^3.2.0",
    "form-serialize": "^0.7.1",
    "is-promise": "^2.1.0",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.2",
    "node-sass": "^3.8.0",
    "node-sass-import-once": "^1.2.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-mdl": "^1.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-preset-stage-2": "^6.11.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "http-server": "^0.9.0",
    "watchify": "^3.7.0"
  }
}
